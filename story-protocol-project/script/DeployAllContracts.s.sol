contract DeployContracts is Script {
    // Story Protocol addresses (these will need to be updated based on the network you're deploying to)
    // These are placeholder addresses - you'll need to replace with actual Story Protocol addresses
    address internal IP_ASSET_REGISTRY = 0x77319B4031e6eF1250907aa00018B8B1c67a244b;
    address internal licenseRegistry = 0x529a750E02d8E2f15649c13D69a465286a780e24;
    address internal LICENSING_MODULE = 0x04fbd8a2e56dd85CFD5500A4A4DfA955B9f1dE6f;
    address internal PIL_TEMPLATE = 0x2E896b0b2Fdb7457499B56AAaA4AE55BCB4Cd316;
    address internal ROYALTY_POLICY_LAP = 0xBe54FB168b3c982b7AaE60dB6CF75Bd8447b390E;
    address internal WIP = 0x1514000000000000000000000000000000000000;

    function run() external {
        uint256 deployerPrivateKey = vm.envUint("PRIVATE_KEY");
        address deployer = vm.addr(deployerPrivateKey);
        
        console2.log("Deploying contracts with account:", deployer);
        console2.log("Account balance:", deployer.balance);

        vm.startBroadcast(deployerPrivateKey);

        // Deploy the main platform contract
        UniversalAssetTokenizationPlatform platform = new UniversalAssetTokenizationPlatform(
            IP_ASSET_REGISTRY,
            LICENSING_MODULE,
            PIL_TEMPLATE,
            ROYALTY_POLICY_LAP,
            WIP
        );

        vm.stopBroadcast();

        // Log deployment addresses
        console2.log("UniversalAssetTokenizationPlatform deployed to:", address(platform));
        console2.log("AssetNFT deployed to:", address(platform.ASSET_NFT()));

        // Write deployment information to file
        _writeDeploymentInfo(address(platform), address(platform.ASSET_NFT()));
    }

    function _writeDeploymentInfo(address platformAddress, address assetNFTAddress) internal {
        string memory deploymentData = string(abi.encodePacked(
            "{\n",
            '  "contracts": {\n',
            '    "UniversalAssetTokenizationPlatform": {\n',
            '      "address": "', vm.toString(platformAddress), '",\n',
            '      "abi": ', _getPlatformABI(), '\n',
            '    },\n',
            '    "AssetNFT": {\n',
            '      "address": "', vm.toString(assetNFTAddress), '",\n',
            '      "abi": ', _getAssetNFTABI(), '\n',
            '    },\n',
            '    "AssetShareToken": {\n',
            '      "note": "Deployed dynamically when assets are created",\n',
            '      "abi": ', _getAssetShareTokenABI(), '\n',
            '    }\n',
            '  },\n',
            '  "deploymentInfo": {\n',
            '    "network": "', vm.toString(block.chainid), '",\n',
            '    "timestamp": "', vm.toString(block.timestamp), '",\n',
            '    "blockNumber": "', vm.toString(block.number), '"\n',
            '  }\n',
            '}'
        ));


        vm.writeFile("./deploy-out/deployments.json", deploymentData);
        console2.log("Deployment info written to deployments.json");
    }

